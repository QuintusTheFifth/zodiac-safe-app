    "lodash.memoize": "^4.1.2",
    "material-ui": "^0.20.2",
    "react": "^17.0.1",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.4",
    "styled-components": "^5.1.1",
    "timeago-react": "^3.0.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.1.3",
    "@types/async-retry": "^1.4.3",
    "@types/jest": "^26.0.14",
    "@types/lodash.memoize": "^4.1.6",
    "@types/node": "^14.6.2",
    "@types/react": "^17.0.0",
    "@types/react-beautiful-dnd": "^13.1.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.2",
    "ganache-core": "2.12.1",
    "prettier": "^2.3.1",
    "react-app-rewired": "^2.1.6",
    "react-scripts": "3.4.0",
    "typescript": "~4.1.3",
    "@zodiac-module/reality": "github:gnosis/zodiac-module-reality#main",
    "@zodiac-module/exit": "github:gnosis/zodiac-module-exit#carlos/packages",
    "@zodiac-module/delay": "github:gnosis/zodiac-modifier-delay#carlos/ethers-version",
    "@zodiac-module/bridge": "github:gnosis/zodiac-module-bridge#main"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-scripts test --watchAll=false",
    "eject": "react-scripts eject",
    "build:modules": "yarn build:module:delay && yarn build:module:reality && yarn build:module:exit && yarn build:module:bridge",
    "build:module:reality": "cd scripts && ./build-zodiac-reality-module.sh",
    "build:module:exit": "cd scripts && ./build-zodiac-exit-module.sh",
    "build:module:delay": "cd scripts && ./build-zodiac-delay-modifier.sh",
    "build:module:bridge": "cd scripts && ./build-zodiac-bridge-module.sh",
    "postinstall": "yarn build:modules"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:jsx-a11y/recommended"
    ],
    "plugins": [
      "jsx-a11y"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
